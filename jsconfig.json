{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "strict": false,
    "allowJs": true,
    "checkJs": false,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules"]
}
